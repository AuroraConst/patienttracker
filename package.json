{
  "name": "patienttracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World (Laminar + Vite): Show"
      }
    ],
    "views": {
      "patient-tracker-view": [
        { "type": "webview","id": "aurora.patientTracker","name": "Patient Tracker","icon": "assets/patient.png" }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {"id": "patient-tracker-view","title": "Patient Tracker","icon": "assets/patient.png"}
      ]
    }
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "dev": "vite",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^10.4.1",
    "prettier": "^2.2.1",
    "vscode-test": "^1.5.0",
    "@scala-js/vite-plugin-scalajs": "^1.0.0",
    "typescript": "^4.9.5",
    "vite": "^5.2.12"

  }
}
